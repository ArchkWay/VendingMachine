<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

   <TextView
       android:id="@+id/tvTitlePurse"
       android:layout_width="match_parent"
       android:layout_height="0dp"
       android:text="@string/your_money"
       android:textAlignment="center"
       app:layout_constraintTop_toTopOf="parent"
       app:layout_constraintBottom_toTopOf="@id/tvCoinOnes"
       />
    <TextView
        android:id="@+id/tvCoinOnes"
        android:text="@string/one_coins"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvTitlePurse"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvCountTwos"
        app:layout_constraintBottom_toTopOf="@id/tvCountOnes"
        />
    <TextView
        android:id="@+id/tvCoinTwos"
        android:text="@string/two_coins"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvTitlePurse"
        app:layout_constraintStart_toEndOf="@id/tvCountOnes"
        app:layout_constraintEnd_toStartOf="@id/tvCountFives"
        app:layout_constraintBottom_toTopOf="@id/tvCountOnes"
        />

    <TextView
        android:id="@+id/tvCoinFives"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/fives"
        app:layout_constraintTop_toBottomOf="@id/tvTitlePurse"
        app:layout_constraintEnd_toStartOf="@id/tvCountTens"
        app:layout_constraintStart_toEndOf="@id/tvCountTwos"
        app:layout_constraintBottom_toTopOf="@id/tvCountOnes"
        />

    <TextView
        android:id="@+id/tvCoinTens"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/tens"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvCountFives"
        app:layout_constraintTop_toBottomOf="@id/tvTitlePurse"
        app:layout_constraintBottom_toTopOf="@id/tvCountOnes"
        />
    <TextView
        android:id="@+id/tvCountOnes"
        android:text="@string/count_example"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvCoinOnes"
        app:layout_constraintBottom_toTopOf="@id/tvTitleDrinks"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvCountTwos"
        />
    <TextView
        android:id="@+id/tvCountTwos"
        android:text="@string/count_example"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvCoinOnes"
        app:layout_constraintBottom_toTopOf="@id/tvTitleDrinks"
        app:layout_constraintStart_toEndOf="@id/tvCountOnes"
        app:layout_constraintEnd_toStartOf="@id/tvCountFives"
        />

    <TextView
        android:id="@+id/tvCountFives"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/count_example"
        app:layout_constraintTop_toBottomOf="@id/tvCoinOnes"
        app:layout_constraintBottom_toTopOf="@id/tvTitleDrinks"
        app:layout_constraintEnd_toStartOf="@id/tvCountTens"
        app:layout_constraintStart_toEndOf="@id/tvCountTwos"

        />

    <TextView
        android:id="@+id/tvCountTens"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/count_example"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvCountFives"
        app:layout_constraintTop_toBottomOf="@id/tvCoinOnes"
        app:layout_constraintBottom_toTopOf="@id/tvTitleDrinks"
        />

    <TextView
        android:id="@+id/tvTitleDrinks"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:text="@string/your_drinks"
        android:textAlignment="center"
        app:layout_constraintTop_toBottomOf="@id/tvCountOnes"
        app:layout_constraintBottom_toTopOf="@id/tvTeas"
        />
    <TextView
        android:id="@+id/tvTeas"
        android:text="@string/teas"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvTitleDrinks"
        app:layout_constraintBottom_toTopOf="@id/tvCountJuices"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvCountTwos"
        />
    <TextView
        android:id="@+id/tvCoffies"
        android:text="@string/coffies"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvTitleDrinks"
        app:layout_constraintBottom_toTopOf="@id/tvCountJuices"
        app:layout_constraintStart_toEndOf="@id/tvCountOnes"
        app:layout_constraintEnd_toStartOf="@id/tvCountFives"
        />

    <TextView
        android:id="@+id/tvCoffiesWithMilk"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/coffies_with_milk"
        app:layout_constraintTop_toBottomOf="@id/tvTitleDrinks"
        app:layout_constraintBottom_toTopOf="@id/tvCountJuices"
        app:layout_constraintEnd_toStartOf="@id/tvCountTens"
        app:layout_constraintStart_toEndOf="@id/tvCountTwos"

        />

    <TextView
        android:id="@+id/tvJuices"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/juices"
        app:layout_constraintTop_toBottomOf="@id/tvTitleDrinks"
        app:layout_constraintBottom_toTopOf="@id/tvCountJuices"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvCountFives"
        />
    <TextView
        android:id="@+id/tvCountTeas"
        android:text="@string/count_example"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvCoffiesWithMilk"
        app:layout_constraintBottom_toTopOf="@id/btnTea"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvCountTwos"
        />
    <TextView
        android:id="@+id/tvCountCoffies"
        android:text="@string/count_example"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvCoffiesWithMilk"
        app:layout_constraintBottom_toTopOf="@id/btnTea"
        app:layout_constraintStart_toEndOf="@id/tvCountOnes"
        app:layout_constraintEnd_toStartOf="@id/tvCountFives"
        />

    <TextView
        android:id="@+id/tvCountCoffiesWithMilk"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/count_example"
        app:layout_constraintTop_toBottomOf="@id/tvCoffiesWithMilk"
        app:layout_constraintBottom_toTopOf="@id/btnTea"
        app:layout_constraintEnd_toStartOf="@id/tvCountTens"
        app:layout_constraintStart_toEndOf="@id/tvCountTwos"

        />

    <TextView
        android:id="@+id/tvCountJuices"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/count_example"
        app:layout_constraintTop_toBottomOf="@id/tvCoffiesWithMilk"
        app:layout_constraintBottom_toTopOf="@id/btnTea"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvCountFives"
        />

    <Button
        android:id="@+id/btnTea"
        android:text="@string/cup_of_tea_10_portions"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tvCountCoffiesWithMilk"
        app:layout_constraintBottom_toTopOf="@id/btnCoffie"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvAmountVMTeas"

        />
    <TextView
        android:id="@+id/tvAmountVMTeas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ten_portions"
        app:layout_constraintTop_toTopOf="@id/btnTea"
        app:layout_constraintBottom_toBottomOf="@id/btnTea"
        app:layout_constraintStart_toEndOf="@id/btnTea"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <Button
        android:id="@+id/btnCoffie"
        android:text="@string/buy_a_cup_of_a_coffie_13_rub_10_portions"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/btnTea"
        app:layout_constraintBottom_toTopOf="@id/btnCoffieWithMilk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvAmountVMTeas"
        />

    <TextView
        android:id="@+id/tvAmountVMCoffies"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ten_portions"
        app:layout_constraintTop_toTopOf="@id/btnCoffie"
        app:layout_constraintBottom_toBottomOf="@id/btnCoffie"
        app:layout_constraintStart_toEndOf="@id/btnCoffie"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <Button
        android:id="@+id/btnCoffieWithMilk"
        android:text="@string/buy_a_cup_of_a_coffie_with_milk_13_rub_10_portions"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/btnCoffie"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvAmountVMCoffiesWithMilk"
        />

    <TextView
        android:id="@+id/tvAmountVMCoffiesWithMilk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ten_portions"
        app:layout_constraintTop_toTopOf="@id/btnCoffieWithMilk"
        app:layout_constraintBottom_toBottomOf="@id/btnCoffieWithMilk"
        app:layout_constraintStart_toEndOf="@id/btnCoffieWithMilk"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <Button
        android:id="@+id/btnJuice"
        android:text="@string/buy_a_cup_of_a_juice_13_rub_10_portions"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/btnCoffieWithMilk"
        app:layout_constraintBottom_toTopOf="@id/tvTitleVendingMoney"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvAmountVMJuices"

        />
    <TextView
        android:id="@+id/tvAmountVMJuices"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ten_portions"
        app:layout_constraintTop_toTopOf="@id/btnJuice"
        app:layout_constraintBottom_toBottomOf="@id/btnJuice"
        app:layout_constraintStart_toEndOf="@id/btnJuice"
        app:layout_constraintEnd_toEndOf="parent"
        />
    <TextView
        android:id="@+id/tvTitleVendingMoney"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/vending_money"
        android:textAlignment="center"
        app:layout_constraintTop_toBottomOf="@id/btnJuice"
        />
    <TextView
        android:id="@+id/tvVMCoinOnes"
        android:text="@string/one_coins"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvTitleVendingMoney"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvCountTwos"
        app:layout_constraintBottom_toTopOf="@id/tvVMCountOnes"
        />
    <TextView
        android:id="@+id/tvVMCoinTwos"
        android:text="@string/two_coins"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvTitleVendingMoney"
        app:layout_constraintStart_toEndOf="@id/tvCountOnes"
        app:layout_constraintEnd_toStartOf="@id/tvCountFives"
        app:layout_constraintBottom_toTopOf="@id/tvVMCountOnes"
        />

    <TextView
        android:id="@+id/tvVMCoinFives"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/fives"
        app:layout_constraintTop_toBottomOf="@id/tvTitleVendingMoney"
        app:layout_constraintEnd_toStartOf="@id/tvCountTens"
        app:layout_constraintStart_toEndOf="@id/tvCountTwos"
        app:layout_constraintBottom_toTopOf="@id/tvVMCountOnes"

        />

    <TextView
        android:id="@+id/tvVMCoinTens"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/tens"
        app:layout_constraintTop_toBottomOf="@id/tvTitleVendingMoney"
        app:layout_constraintBottom_toTopOf="@id/tvVMCountOnes"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvCountFives"
        />
    <TextView
        android:id="@+id/tvVMCountOnes"
        android:text="@string/count_example"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvVMCoinOnes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvCountTwos"
        app:layout_constraintBottom_toTopOf="@id/btnIncertOne"
        />
    <TextView
        android:id="@+id/tvVMCountTwos"
        android:text="@string/count_example"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/tvVMCoinOnes"
        app:layout_constraintStart_toEndOf="@id/tvCountOnes"
        app:layout_constraintEnd_toStartOf="@id/tvCountFives"
        app:layout_constraintBottom_toTopOf="@id/btnIncertOne"
        />

    <TextView
        android:id="@+id/tvVMCountFives"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/count_example"
        app:layout_constraintTop_toBottomOf="@id/tvVMCoinOnes"
        app:layout_constraintEnd_toStartOf="@id/tvCountTens"
        app:layout_constraintStart_toEndOf="@id/tvCountTwos"
        app:layout_constraintBottom_toTopOf="@id/btnIncertOne"

        />

    <TextView
        android:id="@+id/tvVMCountTens"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/count_example"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvCountFives"
        app:layout_constraintTop_toBottomOf="@id/tvVMCoinOnes"
        app:layout_constraintBottom_toTopOf="@id/btnIncertOne"
        />

    <Button
        android:id="@+id/btnIncertOne"
        android:text="@string/incert_1_rub"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="@id/tvTitleAmountIncert"
        app:layout_constraintTop_toBottomOf="@id/tvVMCountOnes"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvCountTwos"
        />
    <Button
        android:id="@+id/btnIncertTwo"
        android:text="@string/incert_2_rub"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tvVMCountOnes"
        app:layout_constraintBottom_toTopOf="@id/tvTitleAmountIncert"
        app:layout_constraintStart_toEndOf="@id/tvCountOnes"
        app:layout_constraintEnd_toStartOf="@id/tvCountFives"
        />

    <Button
        android:id="@+id/btnIncertFive"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/incert_5_rub"
        app:layout_constraintTop_toBottomOf="@id/tvVMCountOnes"
        app:layout_constraintBottom_toTopOf="@id/tvTitleAmountIncert"
        app:layout_constraintEnd_toStartOf="@id/tvCountTens"
        app:layout_constraintStart_toEndOf="@id/tvCountTwos"

        />

    <Button
        android:id="@+id/btnIncertTen"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/incert_10_rub"
        app:layout_constraintTop_toBottomOf="@id/tvVMCountOnes"
        app:layout_constraintBottom_toTopOf="@id/tvTitleAmountIncert"
        app:layout_constraintStart_toEndOf="@id/tvCountFives"
        app:layout_constraintEnd_toEndOf="parent"
        />

    <TextView
        android:id="@+id/tvTitleAmountIncert"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="@string/you_ve_incerted"
        android:textAlignment="viewEnd"
        android:textSize="24sp"
        app:layout_constraintTop_toBottomOf="@id/btnIncertOne"
        app:layout_constraintBottom_toTopOf="@id/btnGetChange"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tvAmountIncerted"
        />
    <TextView
        android:id="@+id/tvAmountIncerted"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="@string/_0_rubles"
        android:textSize="35sp"
        app:layout_constraintTop_toBottomOf="@id/btnIncertOne"
        app:layout_constraintBottom_toTopOf="@id/btnGetChange"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvTitleAmountIncert"
        />
    <Button
        android:id="@+id/btnGetChange"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:text="@string/get_change"
        app:layout_constraintTop_toBottomOf="@id/tvAmountIncerted"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"


        />


</android.support.constraint.ConstraintLayout>